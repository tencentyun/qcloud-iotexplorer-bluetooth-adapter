{
  "include": [
    "src",
    "global.d.ts"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es2017"
    ],
    "outDir": "./dist",
    //    "jsx": "react",
    //    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true,
    "importHelpers": true,
    "sourceMap": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "types": [
      "miniprogram-api-typings"
    ],
    "forceConsistentCasingInFileNames": true,
    "allowJs": true
  }
}
